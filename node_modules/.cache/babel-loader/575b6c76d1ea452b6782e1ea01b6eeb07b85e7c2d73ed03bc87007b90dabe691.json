    <div class='btns text-center d-flex'>\r\n            <button @click=\"about\" class='about-btn' data-alt='<i class=\"fa-solid fa-arrow-left\"></i> Mis proyectos'>Sobre mí <i class=\"fa-solid fa-arrow-right\"></i></button>\r\n            <a target=\"_blank\" download=\"Julián Lora CV\" href=\"/archivos/spa.pdf\"><button class='about-btn' >Descargar CV</button></a>\r\n        </div>\r\n        \r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\nname: 'AppProfile',\r\nmethods:{\r\n    about(){\r\n        const element = document.querySelector('.profile-cont')\r\n        element.classList.toggle('expanded')\r\n        const btn = document.querySelector('.about-btn')\r\n        let alt = btn.getAttribute('data-alt')\r\n        btn.setAttribute('data-alt', btn.innerHTML)\r\n        btn.innerHTML = alt\r\n      }\r\n}\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.profile-cont{\r\n    padding: 0;\r\n    position: fixed;\r\n    width: 230px;\r\n    /* width: 100%; */\r\n    height: 100%;\r\n    transition: width 0.5s;\r\n    background-color: #fff;\r\n    z-index: 2;\r\n}\r\n\r\n.expanded .btns{\r\n    di\r\n}\r\n\r\n#profile{\r\n    align-items: center;\r\n    /* transform: translateY(-180px); */\r\n    background-color: #fff;\r\n    box-shadow: 10px -6px 15px rgba(0, 0, 0, 0.1);\r\n    height: 100%;\r\n}\r\n\r\n#profile-img{\r\n    width: 160px;\r\n    height: 160px;\r\n    background-image: url(../assets/photo_4927435955602828172_x.jpg);\r\n    background-position: center; \r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    border-radius: 50%;\r\n    margin-top: 50px;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.about-btn{\r\n    background-color: #000;\r\n    color: #fff;\r\n    width: 150px;\r\n    height: 45px;\r\n    transition: all 0.3s;\r\n    margin-top: 20px\r\n}\r\n\r\n.about-btn:hover{\r\n    background-color: #fff;\r\n    color: #000;\r\n}\r\n.expanded{\r\n    width: 100%;\r\n}\r\n\r\n#info{\r\n    display: none;\r\n}\r\n\r\n.expanded #info{\r\n    display: block;\r\n    max-width: 600px;\r\n    text-align: justify;\r\n    margin-top: 20px;\r\n}\r\n\r\n.redes a{\r\n    padding-left: 5px;\r\n    padding-right: 5px;\r\n    margin-top: 10px;\r\n    text-decoration: none;\r\n    /* color: #000; */\r\n    transition: all 0.2s;\r\n}\r\n\r\n.redes a:hover{\r\n    color: #000;\r\n}\r\n\r\n@media (max-width: 768px){\r\n    .profile-cont{\r\n        display: none;\r\n    }\r\n}\r\n</style>"],"mappings":"AA6BA,eAAe;EACfA,IAAI,EAAE,YAAY;EAClBC,OAAO,EAAC;IACJC,KAAKA,CAAA,EAAE;MACH,MAAMC,OAAM,GAAIC,QAAQ,CAACC,aAAa,CAAC,eAAe;MACtDF,OAAO,CAACG,SAAS,CAACC,MAAM,CAAC,UAAU;MACnC,MAAMC,GAAE,GAAIJ,QAAQ,CAACC,aAAa,CAAC,YAAY;MAC/C,IAAII,GAAE,GAAID,GAAG,CAACE,YAAY,CAAC,UAAU;MACrCF,GAAG,CAACG,YAAY,CAAC,UAAU,EAAEH,GAAG,CAACI,SAAS;MAC1CJ,GAAG,CAACI,SAAQ,GAAIH,GAAE;IACpB;EACN;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}