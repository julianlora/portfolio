   </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport AppProyecto from './AppProyecto.vue';\r\n// import AppProyectoMosaico from './AppProyectoMosaico.vue';\r\n// import AOS from 'aos';\r\n// import 'aos/dist/aos.css';\r\nimport OverlayScrollbars from 'overlayscrollbars';\r\nimport 'overlayscrollbars/css/OverlayScrollbars.css';\r\n\r\nexport default {\r\n    name: 'AppProyectos',\r\n    components:{\r\n        AppProyecto,\r\n        // AppProyectoMosaico,\r\n    },\r\n    data() {\r\n        return {\r\n            vista: 'list',\r\n            refresh: 0,\r\n        };\r\n    },\r\n    mounted() {\r\n        OverlayScrollbars(document.querySelector('#scrollable'), {\r\n            className: 'os-theme-round-dark',\r\n            overflowBehavior : {\r\n                x : \"hidden\",\r\n                y : \"scroll\"\r\n            },\r\n            callbacks: {\r\n                onScroll: function() {\r\n                    handleScroll();\r\n                }\r\n            }\r\n        });\r\n\r\n        const scrollbar = document.querySelector('.os-scrollbar-vertical .os-scrollbar-track')\r\n        scrollbar.style.position = 'relative'\r\n        var fechas = [];\r\n        var conteoFechas = {};\r\n        document.querySelectorAll('.proyecto').forEach((proyecto) => {\r\n            let fecha = proyecto.getAttribute('data-fecha');\r\n            let fechaNumero = parseInt(fecha);\r\n\r\n            if (!isNaN(fechaNumero)) {\r\n                fechas.push(fechaNumero);\r\n\r\n                if (conteoFechas[fechaNumero]) {\r\n                    conteoFechas[fechaNumero]++;\r\n                } else {\r\n                    conteoFechas[fechaNumero] = 1;\r\n                }\r\n            }\r\n        });\r\n\r\n        console.log('Fechas:', fechas);\r\n        console.log('Conteo de Fechas:', conteoFechas);\r\n        const cantidad = fechas.length\r\n        let conteoFechasArray = Object.entries(conteoFechas);\r\n        conteoFechasArray.sort((a, b) => b[0] - a[0]);\r\n        var distance = 0;\r\n        conteoFechasArray.forEach(([fecha, conteo]) => {\r\n            let loc = Math.round((conteo / cantidad) * 100);\r\n            distance += loc;\r\n            const element = `\r\n                <div class='scrollbar-point' style='top:calc(${distance}% - 18px)'>\r\n                ${fecha}\r\n                </div>\r\n            `;\r\n            scrollbar.insertAdjacentHTML('beforeend', element);\r\n        });\r\n        \r\n        const handle = document.querySelector('.os-scrollbar-vertical .os-scrollbar-handle')\r\n        var element = `\r\n        <div class='scrollbar-tag'>\r\n            ${Math.max(...fechas)}\r\n        </div>\r\n        `\r\n        handle.insertAdjacentHTML('beforeend', element)\r\n\r\n        const points = document.querySelectorAll('.scrollbar-point');\r\n        function handleScroll() {\r\n            console.log('scrolling');\r\n\r\n            points.forEach((point) => {\r\n                const rect1 = document.querySelector('.scrollbar-point').getBoundingClientRect();\r\n                const rect2 = document.querySelector('.os-scrollbar-vertical .os-scrollbar-handle').getBoundingClientRect();\r\n                const distanciaY = rect2.top - rect1.top\r\n                console.log('Distancia vertical (Y): ' + distanciaY + 'px');\r\n            });\r\n\r\n            \r\n        }\r\n        \r\n    },\r\n    methods: {\r\n        displayChange(event, type) {\r\n            console.log(type);\r\n            if(!event.target.classList.contains('selected')){\r\n                document.querySelectorAll('.display-icon').forEach((icon) => {\r\n                    icon.classList.toggle('selected');\r\n                });\r\n                // document.querySelectorAll('.proyecto').forEach((proyecto) => {\r\n                //     proyecto.style.display = 'none';\r\n                // });\r\n            }\r\n            this.vista = type;\r\n            this.refresh += 1;\r\n        },\r\n        \r\n    }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n\r\n.seccion-titulo{\r\n    font-size: 30px;\r\n    font-weight: 600;\r\n    padding-bottom: 25px;\r\n}\r\n\r\n.proyecto-cont{\r\n    padding: 20px 25px 0px 25px;\r\n}\r\n\r\n.proyectos{\r\n    /* border:1px solid red; */\r\n    max-height: calc(100vh - 150px);\r\n    /* overflow-y: scroll; */\r\n}\r\n\r\n.display{\r\n    align-items: center;\r\n}\r\n\r\n.display img{\r\n    height: 30px;\r\n    opacity: 0.5;\r\n    transition: all 0.2s;\r\n}\r\n\r\n.display img:hover{\r\n    opacity: 1;\r\n}\r\n\r\n.selected{\r\n    opacity: 1 !important;\r\n}\r\n\r\n.row{\r\n    margin-left: 0 !important;\r\n}\r\n\r\n\r\n</style>"],"mappings":";;;EACSA,KAA4B,EAA5B;IAAA;EAAA;AAA4B;;EAExBC,KAAK,EAAC;AAAe;;EAmBjBA,KAAK,EAAC;AAA+C;gEACtDC,mBAAA,CAAoB,aAAf,WAAS;;EACTD,KAAK,EAAC;AAAgB;;EAM1BE,EAAE,EAAC;AAAY;;;uBA7B5BC,mBAAA,CAoGM,OApGNC,UAoGM,GAnGFC,mBAAA,eAAkB,EAClBJ,mBAAA,CAiGM,OAjGNK,UAiGM,GAhGFD,mBAAA,2qBAiBU,EACVJ,mBAAA,CAMM,OANNM,UAMM,GALFC,UAAoB,EACpBP,mBAAA,CAGM,OAHNQ,UAGM,GAFFR,mBAAA,CAAqG;IAAhGD,KAAK,EAAC,qBAAqB;IAACU,GAAG,EAAC,sBAAsB;IAAEC,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,QAAA,CAAAC,aAAa,CAACF,MAAM;MACxFZ,mBAAA,CAAuG;IAAlGD,KAAK,EAAC,4BAA4B;IAACU,GAAG,EAAC,mBAAmB;IAAEC,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,QAAA,CAAAC,aAAa,CAACF,MAAM;UAIpGZ,mBAAA,CAqEM,OArENe,UAqEM,I,cApEFb,mBAAA,CAmEM;IAnEDH,KAAK,EAAC,eAAe;IAAEiB,GAAG,EAAEC,KAAA,CAAAC,OAAO;IAAE,UAAQ,EAAC;MAC/CC,YAAA,CAScC,sBAAA;IARVC,MAAM,EAAC,YAAY;IACnBC,MAAM,EAAC,wBAAwB;IAC/BC,WAAW,EAAC,sTAAsT;IAClUC,IAAI,EAAC,2CAA2C;IAC/CC,WAAW,EAAE,yEAAqE;IAClFC,KAAK,EAAET,KAAA,CAAAS,KAAK;IACbC,KAAK,EAAC;sCAIVR,YAAA,CAScC,sBAAA;IARVC,MAAM,EAAC,eAAe;IACtBC,MAAM,EAAC,kBAAkB;IACzBC,WAAW,EAAC,kHAAkH;IAC9HC,IAAI,EAAC,qBAAqB;IACzBC,WAAW,EAAE,mEAA+D;IAC5EC,KAAK,EAAET,KAAA,CAAAS,KAAK;IACbC,KAAK,EAAC;sCAIVR,YAAA,CAScC,sBAAA;IARVC,MAAM,EAAC,kBAAkB;IACzBC,MA